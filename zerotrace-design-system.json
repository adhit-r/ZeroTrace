        "position": "fixed",
        "left": "50%",
        "top": "50%",
        "zIndex": "50",
        "width": "100%",
        "maxWidth": "32rem",
        "transform": "translate(-50%, -50%)",
        "gap": "1rem",
        "border": "3px solid rgba(0, 0, 0, 1)",
        "backgroundColor": "rgba(255, 255, 255, 1)",
        "padding": "1.5rem",
        "boxShadow": "12px 12px 0px 0px rgba(0, 0, 0, 1)",
        "borderRadius": "0.25rem"
