          "boxShadow": "4px 4px 0px 0px rgba(0, 0, 0, 1)",
          "textTransform": "uppercase",
          "letterSpacing": "0.025em",
          "hover": {
            "transform": "translate(2px, 2px)",
            "boxShadow": "2px 2px 0px 0px rgba(0, 0, 0, 1)"
